                                        <dxm:ASPxMenu ID="menu" runat="server" ClientInstanceName="menu" ItemAutoWidth="False"
                                            Width="100%">
                                            <ClientSideEvents ItemClick="function(s, e) {
        switch (e.item.name)
        {
			case &quot;iniciar&quot;:
			case &quot;encerrar&quot;:
                DefineInicioTerminoReuniao(e.item.name, GetCurrentDateAndTime());
				break;
            case &quot;primeiro&quot;:
                MoveFirstRow();
                break;
            case &quot;anterior&quot;:
                MovePrevRow();
                break;
            case &quot;proximo&quot;:
                MoveNextRow();
                break;
            case &quot;ultimo&quot;:
                MoveLastRow();
                break;
            case &quot;zoomout&quot;:
                AjustaFonte(-5);
                break;
            case &quot;zoomin&quot;:
                AjustaFonte(5);
                break;
            case &quot;fullscreen&quot;:
				var $div = $('#dvBarraTopo');
				$div.toggle('blind', function() {
        		var urlImage = e.item.GetImageUrl();
	    		if (urlImage.indexOf('fullscreen.png') == -1)
		    		callbackPanel.PerformCallback(0);
	    		else
		    		callbackPanel.PerformCallback($div.height());
    			});
                break;
			default:
				break;
        }
}" PopUp="function(s, e) {
	switch(e.item.name){
		case &quot;iniciar&quot;:
			popup.ShowWindow(popup.GetWindowByName('winInicio'));
			break;
		case &quot;encerrar&quot;:
			popup.ShowWindow(popup.GetWindowByName('winTermino'));
			break;
	}
}" />
