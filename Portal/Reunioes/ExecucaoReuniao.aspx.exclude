    <script type="text/javascript">
        $(document).ready(function () {
            
            if (hfGeral.Contains("AjusteFonte")) {
                var ajuste = hfGeral.Get("AjusteFonte");
                //AjustaFonte(ajuste);
            }
            $(document).keydown(function (e) {
                //  end     = 35
                //  home    = 36
                //  left    = 37
                //  up      = 38
                //  right   = 39
                //  down    = 40
                var focusedRowIndex = gvListaItrens.GetFocusedRowIndex();

                switch (e.keyCode) {
                    case 35:
                        MoveLastRow();
                        break;
                    case 36:
                        MoveFirstRow();
                        break;
                    case 37:
                    case 38:
                        MovePrevRow();
                        break;
                    case 39:
                    case 40:
                        MoveNextRow();
                        break;
                }
            });
        });

        function GetCurrentDateAndTime() {
            var today = new Date();
            var dd = today.getDate();
            var MM = today.getMonth() + 1;
            var yyyy = today.getFullYear();
            var hh = today.getHours();
            var mm = today.getMinutes();

            if (dd < 10)
                dd = '0' + dd;
            if (MM < 10)
                MM = '0' + MM;
            if (hh < 10)
                hh = '0' + hh;
            if (mm < 10)
                mm = '0' + mm;

            today = dd + '/' + MM + '/' + yyyy + ' ' + hh + ':' + mm;

            return today;
        }

        function DefineInicioTerminoReuniao(acao, dataHora) {
            if (confirm("Deseja definir a data e hora de " + ((acao == 'iniciar') ? 'início' : 'término') + " da reunião como sendo '" + dataHora + "'?"))
                callback.PerformCallback(acao + ';' + dataHora);
        }

        function MoveNextRow() {
            var focusedRowIndex = gvListaItrens.GetFocusedRowIndex();
            if (focusedRowIndex < gvListaItrens.cpRowCount - 1)
                gvListaItrens.SetFocusedRowIndex(focusedRowIndex + 1);
        }

        function MovePrevRow() {
            var focusedRowIndex = gvListaItrens.GetFocusedRowIndex();
            if (focusedRowIndex > 0)
                gvListaItrens.SetFocusedRowIndex(focusedRowIndex - 1);
        }

        function MoveFirstRow() {
            var focusedRowIndex = gvListaItrens.GetFocusedRowIndex();
            if (focusedRowIndex > 0)
                gvListaItrens.SetFocusedRowIndex(0);
        }

        function MoveLastRow() {
            var focusedRowIndex = gvListaItrens.GetFocusedRowIndex();
            var indexOfLast = gvListaItrens.cpRowCount - 1;
            if (focusedRowIndex < indexOfLast)
                gvListaItrens.SetFocusedRowIndex(indexOfLast);
        }

        function OnGetRowValues(value) {
            window.frames["_frmConteudo"].location = value;
        }

        function AjustaFonte(ajuste) {
            window.frames[0].window.frames[0].window.AjustaFonte(ajuste);
        }

    </script>
