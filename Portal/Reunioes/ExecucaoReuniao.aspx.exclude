                    <ClientSideEvents CallbackComplete="function(s, e) {
    var acao = e.parameter.split(';')[0];
	menu.GetItemByName(acao).GetItem(0).SetVisible(false);
	menu.GetItemByName(acao).SetVisible(false);
	if(acao == 'iniciar'){
		menu.GetItemByName('encerrar').SetVisible(true);
		menu.GetItemByName('encerrar').GetItem(0).SetVisible(true);
		popup.HideWindow(popup.GetWindowByName('winInicio'));
	}
	else
		popup.HideWindow(popup.GetWindowByName('winTermino'));
}" />
