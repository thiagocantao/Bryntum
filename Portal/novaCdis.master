        async function getNotificationsFromApi(token) {

            var authorization = "Bearer " + token;

            var primPressIconNotif = "false";
            var nbrJustAccessed = sessionStorage.getItem('nbr_justAccessed');
            if (nbrJustAccessed != null && nbrJustAccessed != undefined) {
                primPressIconNotif = sessionStorage.getItem('nbr_justAccessed');
            }
            
            var listaNotificacoes = sessionStorage.getItem('listaNotificacoes');

            if (listaNotificacoes == "null" || listaNotificacoes == null || listaNotificacoes == undefined) {
               $.ajax({
                   "url": urlWSnewBriskBase + "/api/v1/plano-acao/info-notificao-usuario?IndicaLoginUsuario=S" + (primPressIconNotif == "true" ? "S" : "N"),
                   "method": "GET",
                   "timeout": 0,
                   "headers": {
                       "Content-Type": "application/json; charset=utf-8",
                       "Authorization": authorization,
                       "cod-entidade-contexto": hfSignalR.Get("CodigoEntidade")
                   },
               }).done(function (data) {
                   if (data != null) {
                       var arrayJSON = data.data;
                       atualizaNotificacoesTela(arrayJSON[0]);
                       sessionStorage.setItem('listaNotificacoes', JSON.stringify(arrayJSON[0]));
                       sessionStorage.setItem('nbr_justAccessed', false);
                       callbackNotificacoes.PerformCallback(JSON.stringify(arrayJSON[0]));
                   }
               }).fail(function (jqXHR) {
                   console.log(jqXHR);
               });
           }
            else {
                atualizaNotificacoesTela(JSON.parse(listaNotificacoes));
           }


        }

        ///Pega o token da localstorage e se o token não estiver lá
        ///ele autentica na api fazendo uma requisição ajax e finalmente pega o token.
        ///Com o token na memória, ele chama a função getNotificationsFromApi
        ///que neste momento ja tem condicoes de atualizar as notificações.
        async function atualizacaoInicialNotificacoesTela() {
            var token = getTokenNBRFromMemory(hfSignalR.Get("TokenAcessoNewBrisk"));
            
            if (token != null) {
                getNotificationsFromApi(token);
            }
        }

