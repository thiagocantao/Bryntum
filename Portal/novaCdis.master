    <script type="text/javascript">

        var urlWSnewBriskBase = hfSignalR.Get("urlWSnewBriskBase");

        function atualizaNotificacoesTela(objetoJson) {

            var indicaIconeRiscoLigado = objetoJson.indicaIconeRiscoS1Ligado;
            var indicaIconeIndicadorLigado = objetoJson.indicaIconeIndicadorLigado;
            var indicaIconeReuniaoLigado = objetoJson.indicaIconeReuniaoLigado;
            var indicaIconeTarefaLigado = objetoJson.indicaIconeTarefaS1Ligado;
            var indicaIconeContratoLigado = objetoJson.indicaIconeContratoLigado;
            var indicaIconePndFinLigado = objetoJson.indicaIconePndFinLigado;
            var indicaIconeAprovTrfLigado = objetoJson.indicaIconeAprovTrfLigado;
            var indicaIconePndWfLigado = objetoJson.indicaIconePndWfLigado;
            var indicaIconeMensagemLigado = objetoJson.indicaIconeMensagemLigado;


            var somaMensagens = 0;
            somaMensagens = somaMensagens + objetoJson.qtdMensagem;
            $('#idULMenssage').empty();
            var myli0 = document.createElement('li');
            myli0.innerHTML = configuraNotificacaoMensagem(objetoJson);
            document.querySelector('#idULMenssage').appendChild(myli0);


            var somaNotificacoes = 0;
            somaNotificacoes = somaNotificacoes + objetoJson.qtdPndWf;
            $('#idULNotificacoes').empty();
            var myli = document.createElement('li');
            myli.innerHTML = configuraNotificacaoPendenciaWf(objetoJson);
            document.querySelector('#idULNotificacoes').appendChild(myli);

            somaNotificacoes = somaNotificacoes + objetoJson.qtdRiscoS1;
            var myli1 = document.createElement('li');
            myli1.innerHTML = configuraNotificacaoRisco(objetoJson);
            document.querySelector('#idULNotificacoes').appendChild(myli1);

            somaNotificacoes = somaNotificacoes + objetoJson.qtdContrato;
            var myli2 = document.createElement('li');
            myli2.innerHTML = configuraNotificacaoContrato(objetoJson);
            document.querySelector('#idULNotificacoes').appendChild(myli2);

            somaNotificacoes = somaNotificacoes + objetoJson.qtdQuestaoS1;
            var myli3 = document.createElement('li');
            myli3.innerHTML = configuraNotificacaoQuestao(objetoJson);
            document.querySelector('#idULNotificacoes').appendChild(myli3);

            somaNotificacoes = somaNotificacoes + objetoJson.qtdReuniao;
            var myli4 = document.createElement('li');
            myli4.innerHTML = configuraNotificacaoReuniao(objetoJson);
            document.querySelector('#idULNotificacoes').appendChild(myli4);

            somaNotificacoes = somaNotificacoes + objetoJson.qtdIndicador;
            var myli5 = document.createElement('li');
            myli5.innerHTML = configuraNotificacaoIndicador(objetoJson);
            document.querySelector('#idULNotificacoes').appendChild(myli5);

            somaNotificacoes = somaNotificacoes + objetoJson.qtdAprovTrf;
            var myli6 = document.createElement('li');
            myli6.innerHTML = configuraAprovacoesTarefas(objetoJson)
            document.querySelector('#idULNotificacoes').appendChild(myli6);

            var somaTarefas = 0;
            somaTarefas = somaTarefas + objetoJson.qtdTarefaS1;
            $('#idULTarefas').empty();
            var myli7 = document.createElement('li');
            myli7.innerHTML = configuraNotificacaoTarefa(objetoJson);
            document.querySelector('#idULTarefas').appendChild(myli7);


            if (indicaIconeRiscoLigado == true ||
                indicaIconePndWfLigado == true ||
                indicaIconeReuniaoLigado == true ||
                indicaIconeContratoLigado == true ||
                indicaIconeIndicadorLigado == true ||
                indicaIconeAprovTrfLigado == true) {
                $("#menuPrincipalExistemNotificacoes").removeClass('nav-notification-gray');
                //Verifica se a classe já esta no objeto
                if ($("#menuPrincipalExistemNotificacoes").hasClass('nav-notification')) {
                    //Obtem a quantidade atual de Notificações plotada na bolinha
                    var objBolinhaNotificacoes = document.getElementById('bolinhaNotificacaoNotificacoes').attributes.qtdNotificacoes;
                    var qtdNotificacoesAtuais = 0;
                    if (objBolinhaNotificacoes != null || objBolinhaNotificacoes != undefined) {
                        qtdNotificacoesAtuais = objBolinhaNotificacoes.value;
                    }
                    //Se a quantidade atual de notificações for menor que a quantidade que veio no json 
                    //então remove a classe para ser reconstruida com a animação
                    if (qtdNotificacoesAtuais < somaNotificacoes) {
                        $("#menuPrincipalExistemNotificacoes").removeClass('animation');
                        $("#menuPrincipalExistemNotificacoes").removeClass('nav-notification');

                    }

                }
                //foi add essa condição para inibir a animação duranta a transição de tela interna
                if (qtdNotificacoesAtuais == 0 || qtdNotificacoesAtuais == somaNotificacoes) {
                    $("#menuPrincipalExistemNotificacoes").addClass('nav-notification');
                } else {
                    setTimeout(function () {
                        $("#menuPrincipalExistemNotificacoes").addClass('nav-notification animation');
                    }, 10);
                }
            }
            else {
                $("#menuPrincipalExistemNotificacoes").removeClass('animation');
                $("#menuPrincipalExistemNotificacoes").removeClass('nav-notification');
                $("#menuPrincipalExistemNotificacoes").addClass('nav-notification-gray');
            }

            if (indicaIconeMensagemLigado == true) {
                $("#menuPrincipalExistemMensagens").show(800);
            }
            else {
                $("#menuPrincipalExistemMensagens").removeClass('animation');
                $("#menuPrincipalExistemMensagens").removeClass('nav-notification');
                $("#menuPrincipalExistemMensagens").addClass('nav-notification-gray');
            }
            if (indicaIconeTarefaLigado == true) {
                $("#menuPrincipalExistemTarefas").removeClass('nav-notification-gray');
                //Verifica se a classe já esta no objeto
                if ($("#menuPrincipalExistemTarefas").hasClass('nav-notification')) {
                    //Obtem a quantidade atual de Tarefas plotada na bolinha
                    var objBolinhaTarefa = document.getElementById('bolinhaNotificacaoTarefas').attributes.qtdTarefas;
                    var qtdTarefasAtuais = 0;
                    if (objBolinhaTarefa != null || objBolinhaTarefa != undefined) {
                        qtdTarefasAtuais = objBolinhaTarefa.value;
                    }
                    //Se a quantidade atual de Tarefas for menor que a quantidade que veio no json 
                    //então remove a classe para ser reconstruida com a animação
                    if (qtdTarefasAtuais < somaTarefas) {
                        $("#menuPrincipalExistemTarefas").removeClass('animation');
                        $("#menuPrincipalExistemTarefas").removeClass('nav-notification');

                    }
                }
                if (qtdTarefasAtuais == 0 || qtdTarefasAtuais == somaTarefas) {
                    $("#menuPrincipalExistemTarefas").addClass('nav-notification');
                } else {
                    setTimeout(function () {
                        $("#menuPrincipalExistemTarefas").addClass('nav-notification animation');
                    }, 10);
                }
            }
            else {
                $("#menuPrincipalExistemTarefas").removeClass('animation');
                $("#menuPrincipalExistemTarefas").removeClass('nav-notification');
                $("#menuPrincipalExistemTarefas").addClass('nav-notification-gray');
            }
            $("#bolinhaNotificacaoMensagem").attr("qtdMensagens", somaMensagens);
            $("#bolinhaNotificacaoNotificacoes").attr("qtdNotificacoes", somaNotificacoes);
            $("#bolinhaNotificacaoTarefas").attr("qtdTarefas", somaTarefas);
            document.getElementById('bolinhaNotificacaoMensagem').innerHTML = somaMensagens > 99 ? "+99" : somaMensagens;
            document.getElementById('bolinhaNotificacaoNotificacoes').innerHTML = somaNotificacoes > 99 ? "+99" : somaNotificacoes;
            document.getElementById('bolinhaNotificacaoTarefas').innerHTML = somaTarefas > 99 ? "+99" : somaTarefas;

            $("#iconeTarefas").attr("data-toggle", "dropdown");
            $("#iconeNotificacoes").attr("data-toggle", "dropdown");
            $("#iconeMensagens").attr("data-toggle", "dropdown");

            $("#iconeTarefas").attr("href", "#");
            $("#iconeNotificacoes").attr("href", "#");
            $("#iconeMensagens").attr("href", "#");
        }

