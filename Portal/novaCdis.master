                    listaNotificacoes = JSON.stringify(objetoJSONTemp);
                }

                if (!$("#menuPrincipalExistemTarefas").is(":hidden")) {
                    $.ajax({
                        "url": urlWSnewBriskBase + "/api/v1/plano-acao/registrar-visualizacao-notificao?indicaGrupoNotificacao=TR1",
                        "method": "POST",
                        "timeout": 0,
                        "headers": {
                            "Content-Type": "application/json; charset=utf-8",
                            "Authorization": "Bearer " + getTokenNBRFromMemory(),
                            "cod-entidade-contexto": hfSignalR.Get("CodigoEntidade")
                        },
                    }).done(function (response) {
                        if (response.success) {
                            $("#menuPrincipalExistemTarefas").removeClass('nav-notification');
                            $("#menuPrincipalExistemTarefas").addClass('nav-notification-gray');
                            callbackNotificacoes.PerformCallback(listaNotificacoes);
                        }
                    });
                }

            });

            window.onresize = function (event) {
                //soma o tamanho da tela e subtrai pelos itens do menu, totalizando o tamanho da tela.
                var tamanhoFrame = 'height:' + ($("body").innerHeight() - ($("#menuPrincipalUsuario").height() + $("#rastroPrincipal").height() + $("#lblTituloTela").height())) + 'px';
                $('#frameMaster').removeAttr("style");
                $('#frameMaster').attr({ 'style': tamanhoFrame });
            };

            $('#ddlVisaoInicial_I').addClass("text-truncate");
            atualizacaoInicialNotificacoesTela();
            conectaSignalR(hfSignalR.Get("TokenAcessoNewBrisk"));
        });

        function RemoveTodasConexoesUsuario(pUserName) {
            connSignalRnewBrisk.invoke("removeConexoesUsuario", pUserName).then(function () {
                console.log("Solicitado a remoção de todas as conexões do usuario: ", userName);
            }).catch(function (err) {
                console.error(err.toString());
            });
        }

